{
  "name": "Hello World!",
  "version": "1.0",
  "manifest_version": 2,
  "description": "My first Chrome extension.",
  "browser_action": {
    "default_icon": "snowflake.png",
    "default_popup": "helloworld.html"
	},
  "content_scripts": [ {
    "js": [ "jquery-2.1.0.min.js", "helloworld.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}